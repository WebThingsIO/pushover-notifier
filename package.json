{
  "name": "pushover-notifier",
  "display_name": "Pushover Notifier",
  "version": "0.0.2",
  "description": "Send notifications through Pushover.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ."
  },
  "homepage": "https://github.com/mozilla-iot/pushover-notifier",
  "author": "Mozilla IoT",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla-iot/pushover-notifier.git"
  },
  "bugs": {
    "url": "https://github.com/mozilla-iot/pushover-notifier/issues"
  },
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "manifest.json",
    "node_modules",
    "pushover-notifier.js"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "type": "notifier",
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "userKey": "",
      "token": ""
    },
    "schema": {
      "type": "object",
      "required": [
        "userKey",
        "token"
      ],
      "properties": {
        "userKey": {
          "description": "Your user key",
          "type": "string"
        },
        "token": {
          "description": "Your app token",
          "type": "string"
        }
      }
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0"
  }
}
